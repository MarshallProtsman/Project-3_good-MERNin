{
  "name": "good-mernin",
  "version": "0.1.0",
  "description": "Group 3 project",
  "private": true,
  "proxy": "http://localhost:3001/",
  "main": "server.js",
  "scripts": {
    "preinstall" : "node gcpSetup.js",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "postinstall": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "node-env-run": "^3.0.2",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@google-cloud/translate": "^4.1.1",
    "axios": "^0.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "moment": "^2.24.0",
    "mongoose": "^5.6.4",
    "postinstall": "^0.4.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "socket.io": "^2.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
